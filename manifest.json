{
    "manifest_version": 2,
    "name": "Wanikanjector",
    "version": "1.0",
    "description": "Replaces English words with Kanji learned on https://www.wanikani.com.",
    "homepage_url": "https://github.com/APlagman/wanikanjector",
    "icons": {
        "48": "icons/wanikanjector-48.png",
        "96": "icons/wanikanjector-96.png"
    },
    "background": {
        "scripts": [
            "src/background/wanikanjector.js"
        ]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["src/content/replace.js"]
        }
    ],
    "browser_action": {
        "default_popup": "html/popup.html"
    },
    "permissions": [

    ],
    "web_accessible_resources": [

    ]
}