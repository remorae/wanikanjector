{
    "manifest_version": 2,
    "name": "Wanikanjector",
    "version": 1.0,
    "description": "Replaces English words with Kanji learned on https://www.wanikani.com.",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["wanikanjector.js"]
        }
    ]
}